{"ts":1359658130886,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"---\nlayout: base\n---\n\n\n<section id=\"edw-news-container\">\n\n<h1 class=\"black\">{{ page.title }}</h1>\n\n{% for article in site.categories.news %}\n\n  {% if article.year == page.year %}\n  <article>\n\n    <h3>{{ article.title }}</h3>\n    <p>\n        <time>{{ article.date | date: \"%d %B %Y\" }}</time>\n    </p>\n    \n    <p class=\"last-child\">\n        {{ article.teaser }}\n        <a href=\"{{ article.url }}\">Read more</a>\n    </p>\n\n    <span class=\"box\"><img src=\"{{ article.image }}\" /></span>\n  </article>\n  {% endif %}\n\n{% endfor %}\n\n</section>\n"]],"start1":0,"start2":0,"length1":0,"length2":538}]],"length":538}
